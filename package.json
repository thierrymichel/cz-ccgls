{
  "name": "cz-ccgls",
  "version": "0.1.1",
  "description": "Commitizen adapter following the Conventional-Changelog with support for Gitmoji and Lerna Scopes.",
  "keywords": [
    "commit",
    "commitizen",
    "changelog",
    "gitmoji",
    "lerna",
    "scope"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "gitmojis.js"
  ],
  "scripts": {
    "commit": "npx git-cz || exit 0",
    "publish": "git push --follow-tags && npm publish",
    "release": "standard-version",
    "start": "node index.js",
    "test": "echo 'Tests need to be setup!'"
  },
  "homepage": "https://github.com/thierrymichel/cz-ccgls#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/thierrymichel/cz-ccgls.git"
  },
  "author": {
    "name": "Thierry Michel",
    "email": "thierry@epic.net",
    "url": "http://epic.net"
  },
  "bugs": {
    "url": "https://github.com/thierrymichel/cz-ccgls/issues"
  },
  "license": "SEE LICENSE IN UNLICENSE",
  "dependencies": {
    "cli-truncate": "^1.1.0",
    "conventional-commit-types": "^2.0.0",
    "fuzzy": "^0.1.3",
    "import-from": "^2.1.0",
    "inquirer": "^5.2.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "lodash.map": "^4.6.0",
    "longest": "^2.0.1",
    "read-pkg-up": "^4.0.0",
    "right-pad": "^1.0.1",
    "wrap-ansi": "^4.0.0"
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "lerna": "^3.3.0",
    "standard-version": "^4.4.0"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  }
}
